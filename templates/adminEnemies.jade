extends layout

block styles
  style
    ul.enemies{
      list-style-type: none;
    }
      

block content
  h2.pageTitle Enemies

  ul.enemies
    for enemy in enemies
      li.enemy
        a.name(href="/admin/enemy/#{enemy._id}") #{enemy.name}
  a.btn.btn-primary(href="/admin/enemy") Add Enemy
